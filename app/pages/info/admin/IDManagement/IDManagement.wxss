.frame {
  position: absolute;
  width: 250px;
  height: 100px;
  background-color: #fff;
  top: 20%;
  left: 10%;
}

.background-color {
  color: #fff;
  background-color: #3cc51f;
  justify-content: center;
  display: flex;
  flex-direction: row;
}

.btn {
  width: 125px;
  height: 30px;
  float: left;
  border-radius: -0px;
  top: 50px;
  font-size: 14px;
}
